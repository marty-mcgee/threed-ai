  "scripts": {
    "______________": "_____________________________________",
    "start":          "vite",
    "build":          "vite build",
    "serve":          "vite preview",
    ":____________0": "_____________________________________",
    "mm":             "node index.js",
    "mm:node":        "node index.js",
    "mm:apollo":      "node src/servers/apollo.js",
    "mm:vite":        "vite",
    "mm:next":        "next",
    "mm:error":       "echo \"Not a thing, sir.\" && exit 1",
    "_____________1": "_____________________________________",
    "node:start":     "node index.js",
    "_____________2": "_____________________________________",
    "vite:start":     "vite",
    "vite:build":     "vite build",
    "vite:serve":     "vite preview",
    "_____________3": "_____________________________________",
    "apollo:start":   "node src/servers/apollo.js",
    "_____________4": "_____________________________________",
    "dev":            "echo \"Node Error: no 'dev' command specified\" && exit 1",
    "_____________5": "_____________________________________",
    "test":           "echo \"Node Error: no 'test' command specified\" && exit 1",
    "_____________6": "_____________________________________",
    "pnpm:lint":      "pnpm next:prettier && pnpm next:eslint",
    "_____________7": "_____________________________________",
    "next:eslint":    "next lint --fix --dir src",
    "next:prettier":  "prettier -l \"./src/**/*.{js,jsx,md}\"",
    "next:dev":       "next dev",
    "next:build":     "next build",
    "next:export":    "EXPORT=true next build && EXPORT=true next export",
    "next:analyze":   "ANALYZE=true next build",
    "next:start":     "next start",
    "_____________X": "_____________________________________"
  },
